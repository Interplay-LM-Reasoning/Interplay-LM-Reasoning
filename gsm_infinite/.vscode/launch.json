{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug datagenerationworker - Basic",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/gsm-infinite/data/realistic/datagenerationworker.py",
            "args": [
                "--numprocs", "2",
                "--opmax", "10",
                "--total", "10",
                "--mod", "-1",
                "--number_range", "5",
                "--target_length", "zero_context",
                "--d", "2",
                "--force",
                "--listoperations", "4"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/gsm-infinite/data/realistic",
            "env": {},
            "justMyCode": false,
            "stopOnEntry": false
        },
